pipeline {
    agent any

    stages {
        stage('git fatch') {
            steps {
                git 'https://github.com/Devops-b1/maven-web-application.git'
            }
        }
        stage('build') {
            steps {
                sh 'mvn clean install'
            }
        }
        stage('else') {
            steps {
                echo 'not working '
            }
        }
    }
}
